Class Node{
  public:
  int data;
  Node * next;
}
void insert_first(int val, Node * head,int size){
  
  Node * new_node= new Node();
  if(size==0){
    new_node->data=val;
    head=new_node;
    tail=new_node;
    size++;
  }
  else{
    new_node->data=val;
    new_node->next=head;
    head=new_node;
    size++;
  }
}
  
  
