Class Node{
  public:
  int data;
  Node * next;
};
void insert_first(int val, Node * head){
  
  Node * new_node= new Node();
  if(head==NULL){
    new_node->data=val;
    new_node->next=NULL;
    head=new_node;
    tail=new_node;
  }
  else{
    new_node->data=val;
    new_node->next=head;
    head=new_node;
  }
  
}
  
  
