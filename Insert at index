Class node{
   public:
   int data;
   node*next;
};
void insert_at_index(int val,int index,node*head){
   node*new_node=new node();
   node*temp=head;
   if(index==0){
        void insert_first(val,head);
   }
   else if(index=length){
         void insert_last(val,head,tail);
   }
   else{
         for(int i=0;i<index-1;i++){
               temp=temp->next;
         }
         new_node->data=val;
         new_node->next=temp->next;
         temp->next=new_node;
   }
}
      
   
